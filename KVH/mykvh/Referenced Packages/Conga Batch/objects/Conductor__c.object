    <validationRules>
        <fullName>Either_Report_or_Query_Id_must_be_suppli</fullName>
        <active>true</active>
        <errorConditionFormula>AND(  ISBLANK(Report_Id__c ), ISBLANK( Query_Id__c ), ISBLANK( Record_Id__c ))</errorConditionFormula>
        <errorDisplayField>Report_Id__c</errorDisplayField>
        <errorMessage>You must supply one of the following: a Report Id, a Query Id or a Record Id.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Not_Both_a_Report_Id_and_a_Query_Id</fullName>
        <active>true</active>
        <errorConditionFormula>( Has_Query_Id__c +  Has_Record_Id__c +  Has_Report_Id__c ) &gt; 1</errorConditionFormula>
        <errorDisplayField>Report_Id__c</errorDisplayField>
        <errorMessage>You must supply *only one* of the following:  a Report Id, a Query Id or a Record Id.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Query_Id_Length</fullName>
        <active>true</active>
        <errorConditionFormula>AND(  Query_Id__c != &quot;&quot; ,

OR( LEN(  Query_Id__c  ) != 15 ,
LEFT(  Query_Id__c , 1) != &quot;a&quot; ))</errorConditionFormula>
        <errorDisplayField>Query_Id__c</errorDisplayField>
        <errorMessage>The Query Id must be a 15-character Salesforce ID from a Conga Query</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Record_Id_Length</fullName>
        <active>true</active>
        <errorConditionFormula>AND( Record_Id__c != &quot;&quot; ,

LEN( Record_Id__c ) != 15)</errorConditionFormula>
        <errorDisplayField>Record_Id__c</errorDisplayField>
        <errorMessage>The Record Id must be a 15-character Salesforce ID</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Report_Id_Length</fullName>
        <active>true</active>
        <errorConditionFormula>AND(  Report_Id__c != &quot;&quot; ,

OR( LEN(  Report_Id__c  ) != 15 ,
LEFT(  Report_Id__c , 3) != &quot;00O&quot; ))</errorConditionFormula>
        <errorDisplayField>Report_Id__c</errorDisplayField>
        <errorMessage>The Report Id must be a 15-character Salesforce ID from a Report</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>URL_Field_Name_Required</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( URL_Field_Name__c )</errorConditionFormula>
        <errorDisplayField>URL_Field_Name__c</errorDisplayField>
        <errorMessage>You must supply the API Name of the field that contains the Composer URL</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Workspace_Id_Length</fullName>
        <active>true</active>
        <errorConditionFormula>AND(  Content_Workspace_Id__c  != &quot;&quot; ,

OR( LEN( Content_Workspace_Id__c ) != 15 ,
LEFT( Content_Workspace_Id__c , 3) != &quot;058&quot; ))</errorConditionFormula>
        <errorDisplayField>Content_Workspace_Id__c</errorDisplayField>
        <errorMessage>The Workspace Id must be a 15-character Salesforce ID from a Content Worksapce.</errorMessage>
    </validationRules>
