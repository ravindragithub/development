    <validationRules>
        <fullName>Changes_cannot_be_made_to_Closed_Periods</fullName>
        <active>true</active>
        <description>The revenue period details cannot be changed once the period is closed</description>
        <errorConditionFormula>(
ISCHANGED(Revenue_Amount_Actual__c) 
|| 
ISCHANGED(Deferred_Balance_Actual__c)
||
ISCHANGED(Revenue_Amount_Projected__c)
|| 
ISCHANGED(Deferred_Balance_Projected__c) 
||
ISCHANGED(Revenue_Period_Start_Date__c) 
||
ISCHANGED(Revenue_Period_End_Date__c) 
)
&amp;&amp;  
Revenue_Period_Closed__c</errorConditionFormula>
        <errorMessage>This revenue period is closed. You cannot add/remove revenue transactions against a closed period.</errorMessage>
    </validationRules>
