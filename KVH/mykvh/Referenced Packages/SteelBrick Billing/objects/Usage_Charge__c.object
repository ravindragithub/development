    <validationRules>
        <fullName>Cannot_change_status_from_Completed</fullName>
        <active>true</active>
        <description>You cannot change the Mediation status of a Completed Usage</description>
        <errorConditionFormula>ISPICKVAL(PRIORVALUE 
(MediationStatus__c), 
&quot;Completed&quot;) &amp;&amp; ISCHANGED(MediationStatus__c)</errorConditionFormula>
        <errorMessage>Cannot change the Mediation status from Completed to other</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_Time</fullName>
        <active>true</active>
        <description>End Date Time should be greater than Start Date Time</description>
        <errorConditionFormula>StartDateTime__c &gt; EndDateTime__c &amp;&amp; 
NOT(ISBLANK(TEXT(Order_Rate_Plan_Charge__r.Usage_Frequency__c)))</errorConditionFormula>
        <errorMessage>End Date Time should be greater than Start Date Time</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_Time_Should_be_Present</fullName>
        <active>true</active>
        <description>End Date Time should be present for Usage Charges for Advance Usage Billing</description>
        <errorConditionFormula>ISNEW() &amp;&amp; ISBLANK(EndDateTime__c) &amp;&amp; 
NOT(ISBLANK(TEXT(Order_Rate_Plan_Charge__r.Usage_Frequency__c)))</errorConditionFormula>
        <errorMessage>End Date Time should be present for Usage Charge</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Quantity_Should_be_present_for_Usage</fullName>
        <active>true</active>
        <description>Quantity should be present for Usage Charges for Advance Usage Billing</description>
        <errorConditionFormula>ISNEW() &amp;&amp; ISBLANK( Quantity__c ) &amp;&amp; 
NOT(ISBLANK(TEXT(Order_Rate_Plan_Charge__r.Usage_Frequency__c)))</errorConditionFormula>
        <errorMessage>Quantity should be present for Usage Charge</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Date_Time_Should_be_Present</fullName>
        <active>true</active>
        <description>Start Date Time should be present for Usage Charges for Advance Usage Billing</description>
        <errorConditionFormula>ISNEW() &amp;&amp; ISBLANK(StartDateTime__c) &amp;&amp; 
NOT(ISBLANK(TEXT(Order_Rate_Plan_Charge__r.Usage_Frequency__c)))</errorConditionFormula>
        <errorMessage>Start Date Time should be present for Usage Charge</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Usage_End_Date_Between_Charge_Dates</fullName>
        <active>true</active>
        <description>Usage End Date should be in between Order Charge Service Activation and Service End Dates (included)
Note : Time is not consider as part of this rule</description>
        <errorConditionFormula>If( (DATEVALUE(EndDateTime__c) &lt; Order_Rate_Plan_Charge__r.Service_Activation_Date__c 
|| 
Order_Rate_Plan_Charge__r.Service_End_Date__c &lt; DATEVALUE(EndDateTime__c)) &amp;&amp; 
NOT(ISBLANK(TEXT(Order_Rate_Plan_Charge__r.Usage_Frequency__c))), true, false)</errorConditionFormula>
        <errorMessage>Usage End Date should be in between Order Charge Service Activation and Service End Dates (included)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Usage_Mediation_Status_Queued</fullName>
        <active>true</active>
        <description>Usage Status should be Queued</description>
        <errorConditionFormula>ISNEW() &amp;&amp; NOT(ISPICKVAL( MediationStatus__c , &quot;Queued&quot;)) &amp;&amp; 
NOT(ISBLANK(TEXT(Order_Rate_Plan_Charge__r.Usage_Frequency__c)))</errorConditionFormula>
        <errorMessage>Usage Mediation Status for new should be Queued</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Usage_Start_Date_Between_Charge_Dates</fullName>
        <active>true</active>
        <description>This will make sure Usage Start Date Time will be in between Order Charge Service Activation and Service End Dates 
Note : Time is not consider as part of this rule</description>
        <errorConditionFormula>If( (DATEVALUE(StartDateTime__c) &lt; Order_Rate_Plan_Charge__r.Service_Activation_Date__c 
|| 
Order_Rate_Plan_Charge__r.Service_End_Date__c &lt; DATEVALUE(StartDateTime__c)) &amp;&amp; 
NOT(ISBLANK(TEXT(Order_Rate_Plan_Charge__r.Usage_Frequency__c))), true, false)</errorConditionFormula>
        <errorMessage>Usage Start Date Time should be in between Order Charge Service Activation and Service End Dates (included)</errorMessage>
    </validationRules>
</CustomObject>
