                <apex:column value="{!rateplan.Product__r.Name}" />
                <apex:column value="{!rateplan.Name}" />
                <apex:column value="{!rateplan.invoiceit_s__Description__c}" />
                
            </apex:pageBlockTable>
       </apex:pageBlockSection>
