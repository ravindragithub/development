          e.stopPropagation();
      });
      
      $('.prev').click(function(e) { 
            //Show/Hide steps
          $('.commonDiv').css('display','none');
          $($(this).attr('href').valueOf('href')).css('display','block');
          var currentDiv = $(this).attr('href').valueOf('href');
          var data = currentDiv.split("#");
