                <!-- script IMO Number Validation Start -->     
                    function validateIMO(){
                        var IsvalidIMO = false;
                        var imo = j$("[id$='vesselIMOnumberIdDetailR']").val();
                        var sum = 0;
                        for (var i = 0, len = imo.length; i < len-1; i++) {
                            sum += (imo[i])*(imo.length-i);
                        }
                        if(sum % 10 == imo[6]){
                            IsvalidIMO = true;
                        }
                        // console.log(IsvalidIMO);
                        return IsvalidIMO;
                    }
