<apex:page >
  <script>
    var url = window.location.href;
    var urlParams = {};
    url.replace(new RegExp("([^?=&]+)(=([^&]*))?", "g"),function($0, $1, $2, $3) {urlParams[$1] = $3;});
    var destination = urlParams.de;
    var serialNumber = urlParams.sn;  
    alert(destination);
    alert(serialNumber);
    </script>
</apex:page>