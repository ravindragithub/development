                        }
                    });
                    if(Isvalid){
                        showLoadingDiv();
                        btnSelectednavigateNext('Airtime_Plans');
                    }else{
                        var msg = 'Please Select Plan Type';
                        showgenericpopup(msg);
                    }
                }
            
            function clearErrorMsg(param){
                var currentcomp  = document.getElementById(param);
