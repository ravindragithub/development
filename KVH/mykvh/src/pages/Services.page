            function ValidateProductName(param){
                var Isvalid ;
                var currentcomp  = document.getElementById(param);
                j$(currentcomp).find('.kvh-show-errormsg-here').empty();
                j$(".productselectedclass").each(function(){
                    if(j$(this).val() == 'Selected'){
                        Isvalid = true;
                    }
                });
                if(j$(currentcomp).find("input[type=radio][name$=bandwidthManagementControl]").length >0){
                    if(Isvalid == undefined)
                        Isvalid = j$(currentcomp).find("input[type=radio][name$=bandwidthManagementControl]").valid();
                    else
                        Isvalid = j$(currentcomp).find("input[type=radio][name$=bandwidthManagementControl]").valid() && Isvalid;
                } 
                
                j$('input[type="radio"].radiovldCust').each(function(){
                    if(j$(this).val() == "true"){
                        Isvalid = true;
                    }
                });
                if((Isvalid && '{!objActivationService}' != null && '{!objActivationService.Product__c}' != null && '{!objActivationService.Product__c}' != '' ) || j$(currentcomp).find("[Id$='Special_Instructions__c']").val() != ''){
                    showLoadingDiv();
                    updateActivationServiceNext();
                }else if(j$(currentcomp).find("input[type=radio][name$=bandwidthManagementControl]:checked").length ==0 && j$(currentcomp).find("input[type=radio][name$=bandwidthManagementControl]").length >0){
                    var errordiv = currenttabtoshowerror();
                    j$(currentcomp).find('.kvh-show-errormsg-here').append(errordiv);
                    var html = "";
                    if(j$(currentcomp).find("input[type=radio][name$=bandwidthManagementControl]").hasClass("error") && (j$(currentcomp).find("input[type=radio][name$=bandwidthManagementControl]").valid() == false)){
                        html += "<li>Select Bandwidth Management Control</li>";
                    }
                    j$(currentcomp).find('.error-box').find('.bullets').html(html);
                    j$(currentcomp).find('.error-box').show();
                }else{
                    var msg = 'Select Rate Plan. If you don\'t see your plan, please enter plan details in the "Special Instructions" field.';
                    showgenericpopup(msg, '50%');
                }
            }
            
