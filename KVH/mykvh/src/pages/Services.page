                } 
                 if(j$(currentcomp).find("input[type=checkbox][name$=Inbound_Applications]").length >0){
                    if(Isvalid == undefined)
                        Isvalid = j$(currentcomp).find("input[type=checkbox][name$=Inbound_Applications]").valid();
                    else
                        Isvalid = j$(currentcomp).find("input[type=checkbox][name$=Inbound_Applications]").valid() && Isvalid;
                } 
                if(j$(currentcomp).find("[Id$='Other_Inbound_Applications__c']").length >0){
                    if(Isvalid == undefined)
                        Isvalid = j$(currentcomp).find("[Id$='Other_Inbound_Applications__c']").valid();
                    else
                        Isvalid = j$(currentcomp).find("[Id$='Other_Inbound_Applications__c']").valid() && Isvalid;
                }
                if(j$(currentcomp).find("[Id$='Inbound_IPs_ranges__c']").length >0){
                    if(Isvalid == undefined)
                        Isvalid = j$(currentcomp).find("[Id$='Inbound_IPs_ranges__c']").valid();
                    else
                        Isvalid = j$(currentcomp).find("[Id$='Inbound_IPs_ranges__c']").valid() && Isvalid;
                }
                if(Isvalid){
                    showLoadingDiv();
                    updateServiceMethodInfo();
                }else{
                    var errordiv = currenttabtoshowerror();
                    j$(currentcomp).find('.kvh-show-errormsg-here').append(errordiv);
                    var html = "";
                    if(j$(currentcomp).find("[Id$='IpServiceContacts']").hasClass("error") && (j$(currentcomp).find("[Id$='IpServiceContacts']").valid() == false)){
                        html += "<li>Select Contact</li>";
                    }
                    if(j$(currentcomp).find("input[type=radio][name$=Regions]").hasClass("error") && (j$(currentcomp).find("input[type=radio][name$=Regions]").valid() == false)){
                        html += "<li>Select Region</li>";
                    }
                    if(j$(currentcomp).find("input[type=checkbox][name$=Inbound_Applications]").hasClass("error") && (j$(currentcomp).find("input[type=checkbox][name$=Inbound_Applications]").valid() == false)){
                        html += "<li>Select Application or \"None\"</li>";
                    }
                    if(j$(currentcomp).find("[Id$='Other_Inbound_Applications__c']").hasClass("error") && (j$(currentcomp).find("[Id$='Other_Inbound_Applications__c']").valid() == false)){
                        html += "<li>Specify Other applications/protocols</li>";
                    }
                    if(j$(currentcomp).find("[Id$='Inbound_IPs_ranges__c']").hasClass("error") && (j$(currentcomp).find("[Id$='Inbound_IPs_ranges__c']").valid() == false)){
                        html += "<li>List IP Addresses or Select \"None\"</li>";
                    }
                    j$(currentcomp).find('.error-box').find('.bullets').html(html);
                    j$(currentcomp).find('.error-box').show();
                }
            }
            
            function validateVoiceLine1(param, param1){
                var Isvalid ;
                var currentcomp  = document.getElementById(param);
                j$(currentcomp).find('.kvh-show-errormsg-here').empty();
                if(j$(currentcomp).find("input[type='radio'][name='voiceline1']").length >0){
                    if(Isvalid == undefined)
                        Isvalid = j$(currentcomp).find("input[type='radio'][name='voiceline1']").valid();
                    else
                        Isvalid = j$(currentcomp).find("input[type='radio'][name='voiceline1']").valid() && Isvalid;
                }
                
                if(j$(currentcomp).find("[Id$='Voice1_User_Country__c']").length >0){
                    if(Isvalid == undefined)
                        Isvalid = j$(currentcomp).find("[Id$='Voice1_User_Country__c']").valid();
                    else
                        Isvalid = j$(currentcomp).find("[Id$='Voice1_User_Country__c']").valid() && Isvalid;
                }
                 if(j$(currentcomp).find("input[type='radio'][name='voiceline2']").length >0){
                    if(Isvalid == undefined)
                        Isvalid = j$(currentcomp).find("input[type='radio'][name='voiceline2']").valid();
                    else
                        Isvalid = j$(currentcomp).find("input[type='radio'][name='voiceline2']").valid() && Isvalid;
                }
                 if(j$(currentcomp).find("[Id$='Voice2_User_Country__c']").length >0){
                    if(Isvalid == undefined)
                        Isvalid = j$(currentcomp).find("[Id$='Voice2_User_Country__c']").valid();
                    else
                        Isvalid = j$(currentcomp).find("[Id$='Voice2_User_Country__c']").valid() && Isvalid;
                }
                if(Isvalid){
                    var voiceline1 ='';
                    var voiceline2 ='';
                    j$("input[type=radio][name=voiceline1]").each(function(){
                        if(j$(this).is(':checked')){
                            voiceline1 = j$(this).val();
                        }
                    });
                    j$("input[type=radio][name=voiceline2]").each(function(){
                        if(j$(this).is(':checked')){
                            voiceline2 = j$(this).val();
                        }
                    });
                    if(param1){
                        showLoadingDiv();
                        console.log('test' + j$("[id$='IP_MobileCast_cell']").length);
                         if(j$("[id$='IP_MobileCast_cell']").length ==1){
                            updateVoiceLine1Record(voiceline1,voiceline2);
                        }
                        else{
                            updateVoiceLine1RecordV3(voiceline1,voiceline2);
                        }
                    }
                    else{
                        if(j$("[id$='IP_MobileCast_cell']").length ==1){
                            updateVoiceLine1Recordsaveexit(voiceline1,voiceline2);
                        }
                        else{
                            updateVoiceLine1RecordV3saveexit(voiceline1,voiceline2);
                        }
                    }
                }else{
                    var errordiv = currenttabtoshowerror();
                    j$(currentcomp).find('.kvh-show-errormsg-here').append(errordiv);
                    var html = "";
                    if(j$(currentcomp).find("input[type=radio][name=voiceline1]").hasClass("error") && (j$(currentcomp).find("input[type=radio][name=voiceline1]").valid() == false)){
                        html += "<li>Select Voice Line 1 Type</li>";
                    } 
                    if(j$(currentcomp).find('[id$=Voice1_User_Country__c]').hasClass("error") && (j$(currentcomp).find("[id$=Voice1_User_Country__c]").valid() == false)){
                        html += "<li>Country For Voice Line 1 Required</li>";
                    }
                    if(j$(currentcomp).find("input[type=radio][name=voiceline2]").hasClass("error") && (j$(currentcomp).find("input[type=radio][name=voiceline2]").valid() == false)){
                        html += "<li>Select Voice Line 2 Type</li>";
                    } 
                    if(j$(currentcomp).find('[id$=Voice2_User_Country__c]').hasClass("error") && (j$(currentcomp).find("[id$=Voice2_User_Country__c]").valid() == false)){
                        html += "<li>Country For Voice Line 2 Required</li>";
                    }
                    if(j$(currentcomp).find("[Id$='Voice1_User_Specified_Code__c']").hasClass("error") && (j$(currentcomp).find("[Id$='Voice1_User_Specified_Code__c']").valid() == false)){
                        html += "<li>Select Area Code</li>";
                    } 
                    j$(currentcomp).find('.error-box').find('.bullets').html(html);
                    j$(currentcomp).find('.error-box').show();
                }
            }
            
            function backOptionalPlans(){
                if(j$("[id$='IP_MobileCast_cell']").length ==1){
                    btnSelectednavigateNext('IP_MobileCast');
                }
                else{
                    btnSelectednavigateNext('Voice_Lines');
                }
            }
            
