.loadingStatus {
	display: none;
	position: fixed;
	top: 50%;
	left: 50%;
	list-style: none;
    z-index: 9999;
}

.loadingStatus .img {
	width: 32px;
	margin: 0 auto;
}

.loadingBox {
	display: none;
	background: #000000;
	opacity: 0.45;
	filter: alpha(opacity=45);
	position: fixed;
	top: 0px;
	left: 0px;
	min-width: 100%;
	min-height: 100%;
	z-index: 9999;
}