({
	showhidepartnersection : function(component, event, helper) {
		var show = event.getParam("show");
        if(show){
            component.set("v.showpartnersection","");
            component.set("v.columnsize","slds-medium-size_1-of-2 slds-large-size_3-of-12");
        }else{
            component.set("v.showpartnersection","display:none;");
            component.set("v.columnsize","slds-medium-size_1-of-1 slds-large-size_6-of-12");
        }
	}
})