({
	miniVSATAction : function(component, event, helper) {
		var urlEvent = $A.get("e.force:navigateToURL");
        urlEvent.setParams({
          "url": component.get("v.miniVSATManager")
        });
        urlEvent.fire();
	},
    LTEManagerAction : function(component, event, helper) {
		var urlEvent = $A.get("e.force:navigateToURL");
        urlEvent.setParams({
          "url": ""
        });
        urlEvent.fire();
	},
    LCAction : function(component, event, helper) {
		var urlEvent = $A.get("e.force:navigateToURL");
        urlEvent.setParams({
          "url": component.get("v.LearningCenter")
        });
        urlEvent.fire();
	},
    SCAction : function(component, event, helper) {
		var urlEvent = $A.get("e.force:navigateToURL");
        urlEvent.setParams({
          "url": "/mykvh-support-landing "
        });
        urlEvent.fire();
	},
    miniVSATActAction : function(component, event, helper) {
		var urlEvent = $A.get("e.force:navigateToURL");
        urlEvent.setParams({
          "url": component.get("v.miniVSATActivation")
        });
        urlEvent.fire();
	},
    LTEActAction : function(component, event, helper) {
		var urlEvent = $A.get("e.force:navigateToURL");
