                    <!-- **  Script Methods And Functions ** -->
                    <script type="text/javascript" language="JavaScript">
                    <!-- script Block for Modal Windows Start -->
                        window.remodalGlobals = {
                        namespace: "modal",
                        defaults: {
                        hashTracking: true
                        }
                        };
                        
                        var options = {hashTracking: false, closeOnOutsideClick: false,closeOnEscape: false};
                        
                        /* To remove duplidate popup window initialized -Start */
                        function removeduplicatepopup(popupID){
                            while(j$(popupID).length >1){
                            j$(popupID).last().remove();
                        }
                                                      while(j$('.remodal-wrapper').find(popupID).length >1){
                            j$('.remodal-wrapper').find(popupID).last().remove();
                        }
                    }
                    /* To remove duplidate popup window initialized -End */
                    
                    
                    /* Activation Corporate button -Start */
                    function buttonfunctionCorporate(param){
                        j$( "#Insttype" ).popup( "open" );
                    }
                    /* Activation Corporate button -End */
                    
                    
                    function submitConfirm(){
                        j$('[data-remodal-id=submitinfo]').find('.remodal-close').click();
                        showLoadingDiv();
                        // orderupdate('Payment_Information');
                    }
                    
                    
                    
                    <!-- script to show error message area -Start -->
                        function currenttabtoshowerror(){
                            var errordiv= "<div class='error-box' id='globalErrorBOX' style='display:none !important'>"; 
                            errordiv += "<h1><img src='/resource/cautionImage' height='20' width='20' />";
                            errordiv += "Please correct the errors indicated below and try again. </h1> <ul class='bullets'></ul> </div>";
                            return errordiv;
                        }
                    <!-- script to show error message area -End -->
                        
                        <!-- script Installation Type Select Button Validation Start --> 
                            function checkBtnSelected(param,param1){
                                var currentcomp  = document.getElementById(param);
                                j$(currentcomp).find('.kvh-show-errormsg-here').empty();
                                if(!(j$(currentcomp).find("input[name=MarketTypeSelection][type='radio']").valid()) && j$(currentcomp).find("input[name=MarketTypeSelection][type='radio']").hasClass("error")){
                                    j$("input[name=MarketTypeSelection][type='radio']").parent().find("label[id=MarketTypeSelection-error]").remove();
                                    var errordiv = currenttabtoshowerror();
                                    j$(currentcomp).find('.kvh-show-errormsg-here').append(errordiv);
                                    
                                    var html = "";
                                    html += "<li>\Select Vessel or Vehicle Type</li>";
                                    j$(currentcomp).find('.error-box').find('.bullets').html(html);
                                    j$(currentcomp).find('.error-box').show();
                                    var errorDivision = j$(currentcomp).find('.error-box').first();
                                    var scrollPos = errorDivision.offset().top;
                                    j$(window).scrollTop(scrollPos);
                                }else{
                                    if(param1){
                                        showLoadingDiv();
                                        if(j$("[id$='IMO_Number_cell']").length>0 )
                                            btnSelectednavigateNext('IMO_Number', j$("input[name=MarketTypeSelection][type='radio']:checked").data('passkey'), '');
                                        else 
                                            btnSelectednavigateNext('Vessel_Details', j$("input[name=MarketTypeSelection][type='radio']:checked").data('passkey'), '');}
                                    else
                                        buttonfunctionCorporate(this);
                                    
                                }
                            }
                    <!-- script Installation Type Select Button Validation End -->
                        
                        <!-- script IMO Number Disable Enable Fields Start -->     
                            function calldisableIMO(){
                                j$('.kvh-show-errormsg-here').empty();
                                if(j$("#IMOnumber").is(':checked')){
                                    if(validmarketnoIMO.indexOf('{!objActivationPlatform.Market__c}')>=0){
                                        showLoadingDiv();
                                        updateimo('Yes');
                                    }
                                }
                                if((j$("#noIMOnumber").is(':checked'))){ 
                                    if(validmarketnoIMO.indexOf('{!objActivationPlatform.Market__c}')>=0){
                                        showLoadingDiv();
                                        updateimo('No');
                                    }
                                }
                            }
                    <!-- script IMO Number Disable Enable Fields End -->
                        
                        <!-- script IMO Number Validation Start -->     
                            function validateIMO(){
                                var IsvalidIMO = false;
                                var imo = j$("[id$='vesselIMOnumberIdDetail']").val();
                                //console.log(j$("[id$='vesselIMOnumberIdDetail']").val());
                                var sum = 0;
                                for (var i = 0, len = imo.length; i < len-1; i++) {
                                    sum += (imo[i])*(imo.length-i);
                                }
                                if(sum % 10 == imo[6]){
                                    IsvalidIMO = true;
                                }
                                // console.log(IsvalidIMO);
                                return IsvalidIMO;
                            }
                    <!--script IMO Number Validation End --> 
                        
                        <!-- script IMO Number Tab JQUERY Validation And navigation Start -->     
                            function validateradio(param,param1){
                                var currentcomp  = document.getElementById(param);
                                j$(currentcomp).find('.kvh-show-errormsg-here').empty();
                                // console.log(currentcomp);
                                if((j$("#IMOnumber").is(':checked'))){
                                    if(param1){
                                        showLoadingDiv();
                                        btnSelectednavigateNext('Vessel_Details', '', 'Yes');}
                                    else
                                        buttonfunctionCorporate(this);
                                }else if((j$("#noIMOnumber").is(':checked'))){
                                    if(param1){
                                        showLoadingDiv();
                                        btnSelectednavigateNext('Vessel_Details', '', 'No');}
                                    else
                                        buttonfunctionCorporate(this);
                                } else{
                                    // console.log('no select');
                                    var errordiv = currenttabtoshowerror();
                                    j$(currentcomp).find('.kvh-show-errormsg-here').append(errordiv);
                                    var html = ""; 
                                    html += "<li>\Select \"I have a Vessel IMO number\"  or  \"I don\'t have a Vessel IMO number\".</li>";
                                    j$(currentcomp).find('.error-box').find('.bullets').html(html);
                                    //  console.log(j$(currentcomp).find('.error-box'));
                                    j$(currentcomp).find('.error-box').show();
                                }
                            }
                    <!--  script IMO Number Tab JQUERY Validation And navigation End --> 
                        
                        <!-- script Vessel Details Tab navigate Next Start -->     
                            function validateVesselDetails(param,param1){
                                var Isvalid;
                                var currentcomp  = document.getElementById(param);
                                //  console.log(currentcomp);
                                // console.log(j$("[id$=Vessel_Details_lbl]").text());
                                j$(currentcomp).find('.kvh-show-errormsg-here').empty();
                                j$(currentcomp).find("[id$=vesselInfoIdDetail]").removeClass('error');
                                j$(currentcomp).find("[id$=vesselMMSIIdDetail]").removeClass('error');
                                if(j$(currentcomp).find("[id$=vesselNameIdDetail]").length ==1){
                                    if(Isvalid == undefined)
                                        Isvalid = j$(currentcomp).find("[id$=vesselNameIdDetail]").valid();
                                    else
                                        Isvalid = j$(currentcomp).find("[id$=vesselNameIdDetail]").valid() && Isvalid;
                                }
                                
                                if(j$(currentcomp).find("[id$=LeisurevesselBuildIdDetail]").length ==1){
                                    if(Isvalid == undefined)
                                        Isvalid = j$(currentcomp).find("[id$=LeisurevesselBuildIdDetail]").valid();
                                    else
                                        Isvalid = j$(currentcomp).find("[id$=LeisurevesselBuildIdDetail]").valid() && Isvalid;
                                }
                                if(j$(currentcomp).find("[id$=vesselIMOnumberIdDetail]").length ==1){
                                    if(Isvalid == undefined)
                                        Isvalid = j$(currentcomp).find("[id$=vesselIMOnumberIdDetail]").valid();
                                    else
                                        Isvalid = j$(currentcomp).find("[id$=vesselIMOnumberIdDetail]").valid() && Isvalid;
                                }
                                if(j$(currentcomp).find("[id$=vesselflagIdDetail]").length ==1){
                                    if(Isvalid == undefined)
                                        Isvalid = j$(currentcomp).find("[id$=vesselflagIdDetail]").valid();
                                    else
                                        Isvalid = j$(currentcomp).find("[id$=vesselflagIdDetail]").valid() && Isvalid;
                                }
                                if(j$(currentcomp).find("[id$=LandvesselCountryIdDetail]").length ==1){
                                    if(Isvalid == undefined)
                                        Isvalid = j$(currentcomp).find("[id$=LandvesselCountryIdDetail]").valid();
                                    else
                                        Isvalid = j$(currentcomp).find("[id$=LandvesselCountryIdDetail]").valid() && Isvalid;
                                }
                                if(j$(currentcomp).find("input[type=radio][name$='Brazilian_Waters_Vessel_c']").length ==1){
                                    if(Isvalid == undefined)
                                        Isvalid = j$(currentcomp).find("input[type=radio][name$='Brazilian_Waters_Vessel_c']").valid();
                                    else
                                        Isvalid = j$(currentcomp).find("input[type=radio][name$='Brazilian_Waters_Vessel_c']").valid() && Isvalid;
                                }
                                if(j$(currentcomp).find("[id$=Distance_between_antenna_and_sea_level__c]").length ==1){
                                    if(Isvalid == undefined)
                                        Isvalid = j$(currentcomp).find("[id$=Distance_between_antenna_and_sea_level__c]").valid();
                                    else
                                        Isvalid = j$(currentcomp).find("[id$=Distance_between_antenna_and_sea_level__c]").valid() && Isvalid;
                                }
                                if(j$(currentcomp).find("[id$=Geograpical_coordinates_of_port_and_site__c]").length ==1){
                                    if(Isvalid == undefined)
                                        Isvalid = j$(currentcomp).find("[id$=Geograpical_coordinates_of_port_and_site__c]").valid();
                                    else
                                        Isvalid = j$(currentcomp).find("[id$=Geograpical_coordinates_of_port_and_site__c]").valid() && Isvalid;
                                }
                                if(j$(currentcomp).find("[id$=Minimum_Latitude__c]").length ==1){
                                    if(Isvalid == undefined)
                                        Isvalid = j$(currentcomp).find("[id$=Minimum_Latitude__c]").valid();
                                    else
                                        Isvalid = j$(currentcomp).find("[id$=Minimum_Latitude__c]").valid() && Isvalid;
                                }
                                if(j$(currentcomp).find("[id$=Maximum_Latitude__c]").length ==1){
                                    if(Isvalid == undefined)
                                        Isvalid = j$(currentcomp).find("[id$=Maximum_Latitude__c]").valid();
                                    else
                                        Isvalid = j$(currentcomp).find("[id$=Maximum_Latitude__c]").valid() && Isvalid;
                                }
                                if(j$(currentcomp).find("[id$=Minimum_Longitude__c]").length ==1){
                                    if(Isvalid == undefined)
                                        Isvalid = j$(currentcomp).find("[id$=Minimum_Longitude__c]").valid();
                                    else
                                        Isvalid = j$(currentcomp).find("[id$=Minimum_Longitude__c]").valid() && Isvalid;
                                }
                                if(j$(currentcomp).find("[id$=Maximum_Longitude__c]").length ==1){
                                    if(Isvalid == undefined)
                                        Isvalid = j$(currentcomp).find("[id$=Maximum_Longitude__c]").valid();
                                    else
                                        Isvalid = j$(currentcomp).find("[id$=Maximum_Longitude__c]").valid() && Isvalid;
                                }
                                if(j$(currentcomp).find("[name$=LeisurevesselIPMobileCastIdDetail]").length >1){
                                    if(Isvalid == undefined)
                                        Isvalid = j$(currentcomp).find("[name$=LeisurevesselIPMobileCastIdDetail]").valid();
                                    else
                                        Isvalid = j$(currentcomp).find("[name$=LeisurevesselIPMobileCastIdDetail]").valid() && Isvalid;
                                }
                                if(j$(currentcomp).find("[id$=vesselLengthIdDetail]").length ==1){
                                    var tempjob = j$(currentcomp).find("[id$=vesselLengthIdDetail]").valid();
                                    if(tempjob == false){
                                        j$(currentcomp).find("[id$=vesselLengthIdDetail]").addClass('kvh-selecterror');
                                    }else{
                                        j$(currentcomp).find("[id$=vesselLengthIdDetail]").removeClass('kvh-selecterror');
                                    }
                                    if(Isvalid == undefined){
                                        Isvalid = tempjob;
                                    }else{
                                        Isvalid = tempjob && Isvalid;
                                    }
                                } 
                                if(j$(currentcomp).find("[id$=vesselPlatformIdDetail]").length ==1){
                                    var tempjob = j$(currentcomp).find("[id$=vesselPlatformIdDetail]").valid();
                                    if(tempjob == false){
                                        j$(currentcomp).find("[id$=vesselPlatformIdDetail]").addClass('kvh-selecterror');
                                    }else{
                                        j$(currentcomp).find("[id$=vesselPlatformIdDetail]").removeClass('kvh-selecterror');
                                    }
                                    if(Isvalid == undefined){
                                        Isvalid = tempjob;
                                    }else{
                                        Isvalid = tempjob && Isvalid;
                                    }
                                } 
                                if(j$(currentcomp).find("[id$=vesselTypeIdDetail]").length ==1){
                                    var tempjob = j$(currentcomp).find("[id$=vesselTypeIdDetail]").valid();
                                    if(tempjob == false){
                                        j$(currentcomp).find("[id$=vesselTypeIdDetail]").addClass('kvh-selecterror');
                                    }else{
                                        j$(currentcomp).find("[id$=vesselTypeIdDetail]").removeClass('kvh-selecterror');
                                    }
                                    if(Isvalid == undefined){
                                        Isvalid = tempjob;
                                    }else{
                                        Isvalid = tempjob && Isvalid;
                                    }
                                } 
                                if(j$(currentcomp).find("[id$=vesselInfoIdDetail]").length ==1){
                                    if(j$(currentcomp).find("[id$=vesselInfoIdDetail]").val() == "" && j$(currentcomp).find("[id$=vesselMMSIIdDetail]").val() == ""){
                                        if(Isvalid == undefined)
                                            Isvalid = j$(currentcomp).find("[id$=vesselInfoIdDetail]").valid();
                                        else
                                            Isvalid = j$(currentcomp).find("[id$=vesselInfoIdDetail]").valid() && Isvalid;
                                        j$(currentcomp).find("[id$=vesselInfoIdDetail]").addClass('error');
                                        j$(currentcomp).find("[id$=vesselMMSIIdDetail]").addClass('error');
                                    }
                                }
                                
                                if(Isvalid){
                                    if(param1){
                                        showLoadingDiv();
                                        if(j$("[id$=Vessel_Details_lbl]").text() != 'Vehicle/Platform Details' )
                                            btnSelectednavigateNext('Onboard_Contact', '', '');
                                        else 
                                            btnSelectednavigateNext('Antenna_System_Support_Contact', '', '');}
                                    else
                                        buttonfunctionCorporate(this);
                                }else{
                                    var errordiv = currenttabtoshowerror();
                                    j$(currentcomp).find('.kvh-show-errormsg-here').append(errordiv);
                                    var html = "";
                                    
                                    if(j$(currentcomp).find('[id$=vesselNameIdDetail]').hasClass("error") && (j$(currentcomp).find("[id$=vesselNameIdDetail]").valid() == false)){
                                        html += "<li>Vessel Name Required</li>";
                                    }
                                    if(j$(currentcomp).find('[id$=vesselflagIdDetail]').hasClass("error") && (j$(currentcomp).find("[id$=vesselflagIdDetail]").valid() == false)){
                                        html += "<li>Select Flag</li>";
                                    }
                                    if(j$(currentcomp).find('[id$=vesselLengthIdDetail]').hasClass("error") && (j$(currentcomp).find("[id$=vesselLengthIdDetail]").valid() == false)){
                                        html += "<li>Select Vessel Length</li>";
                                    }
                                    if(j$(currentcomp).find('[id$=vesselTypeIdDetail]').hasClass("error") && (j$(currentcomp).find("[id$=vesselTypeIdDetail]").valid() == false)){
                                        if(j$(currentcomp).find('[id$=vesselTypeIdDetail]').attr('id').indexOf('LandvesselTypeIdDetail') > -1)
                                            html += "<li>Select Vehicle/Platform Type</li>";
                                        else
                                            html += "<li>Select Vessel Type</li>";
                                    }
                                    
                                    if(j$(currentcomp).find('[id$=vesselPlatformIdDetail]').hasClass("error") && (j$(currentcomp).find("[id$=vesselPlatformIdDetail]").valid() == false)){
                                        if(j$(currentcomp).find('[id$=vesselPlatformIdDetail]').attr('id').indexOf('LandvesselPlatformIdDetail') > -1)
                                            html += "<li>Select Vehicle/Platform Detail</li>";
                                        else
                                            html += "<li>Select Vessel Detail</li>";
                                    }
                                    if(j$(currentcomp).find('[id$=LandvesselCountryIdDetail]').hasClass("error") && (j$(currentcomp).find("[id$=LandvesselCountryIdDetail]").valid() == false)){
                                        html += "<li>Select Country of Registration</li>";
                                    }
                                    if(j$(currentcomp).find('[id$=vesselIMOnumberIdDetail]').hasClass("error") && (j$(currentcomp).find("[id$=vesselIMOnumberIdDetail]").valid() == false)){
                                        html += "<li>Enter Vessel IMO Number</li>";
                                    }
                                    if(j$(currentcomp).find('[id$=LeisurevesselBuildIdDetail]').hasClass("error") && (j$(currentcomp).find("[id$=LeisurevesselBuildIdDetail]").valid() == false)){
                                        html += "<li>Boat Builder/Shipyard required</li>";
                                    }
                                    if(j$(currentcomp).find("[id$=vesselInfoIdDetail]").val() == "" && j$(currentcomp).find("[id$=vesselMMSIIdDetail]").val() == ""){
                                        html += "<li>Enter Radio Call Sign or MMSI</li>";
                                    }
                                    if(j$(currentcomp).find('[name$=LeisurevesselIPMobileCastIdDetail]').hasClass("error") && (j$(currentcomp).find("[name$=LeisurevesselIPMobileCastIdDetail]").valid() == false)){
                                        html += "<li>Please indicate whether this vessel is a charter boat/yacht</li>";
                                    }
                                    if(j$(currentcomp).find("input[type=radio][name$='Brazilian_Waters_Vessel_c']").hasClass("error") && (j$(currentcomp).find("input[type=radio][name$='Brazilian_Waters_Vessel_c']").valid() == false)){
                                        html += "<li>Brazilian Waters Vessel Required</li>";
                                    }
                                    if(j$(currentcomp).find('[id$=Distance_between_antenna_and_sea_level__c]').hasClass("error") && (j$(currentcomp).find("[id$=Distance_between_antenna_and_sea_level__c]").valid() == false)){
                                        html += "<li>Distance between antenna and sea level Required</li>";
                                    }
                                    if(j$(currentcomp).find('[id$=Geograpical_coordinates_of_port_and_site__c]').hasClass("error") && (j$(currentcomp).find("[id$=Geograpical_coordinates_of_port_and_site__c]").valid() == false)){
                                        html += "<li>Geograpical coordinates of port and site Required</li>";
                                    }
                                    if(j$(currentcomp).find('[id$=Minimum_Latitude__c]').hasClass("error") && (j$(currentcomp).find("[id$=Minimum_Latitude__c]").valid() == false)){
                                        html += "<li>Minimum Latitude Required</li>";
                                    }
                                    if(j$(currentcomp).find('[id$=Maximum_Latitude__c]').hasClass("error") && (j$(currentcomp).find("[id$=Maximum_Latitude__c]").valid() == false)){
                                        html += "<li>Maximum Latitude Required</li>";
                                    }
                                    if(j$(currentcomp).find('[id$=Minimum_Longitude__c]').hasClass("error") && (j$(currentcomp).find("[id$=Minimum_Longitude__c]").valid() == false)){
                                        html += "<li>Minimum Longitude Required</li>";
                                    }
                                    if(j$(currentcomp).find('[id$=Maximum_Longitude__c]').hasClass("error") && (j$(currentcomp).find("[id$=Maximum_Longitude__c]").valid() == false)){
                                        html += "<li>Maximum Longitude Required</li>";
                                    }
                                    j$(currentcomp).find('.error-box').find('.bullets').html(html);
                                    j$(currentcomp).find('.error-box').show();
                                }
                                
                            }
                    <!-- script Vessel Details Tab navigate Next End --> 
                        
                        // Validator script //
                        function validateError(param,param1){
                            var Isvalid;
                            var currentcomp = j$("[id$="+param+"]");
                            var skipvalidationVar = j$(currentcomp).find("[id$=skipValidation]").is(':checked');
                            //   console.log('==>' +skipvalidationVar);
                            if(!skipvalidationVar){
                                currentcomp = j$("[id$="+param+"]");
                                j$(currentcomp).find('.kvh-show-errormsg-here').empty();
                                if(j$(currentcomp).find("[id$=firstname]").length ==1){
                                    if(Isvalid == undefined)
                                        Isvalid = j$(currentcomp).find("[id$=firstname]").valid();
                                    else
                                        Isvalid = j$(currentcomp).find("[id$=firstname]").valid() && Isvalid;
                                }
                                if(j$(currentcomp).find("[id$=lastname]").length ==1){
                                    if(Isvalid == undefined)
                                        Isvalid = j$(currentcomp).find("[id$=lastname]").valid();
                                    else
                                        Isvalid = j$(currentcomp).find("[id$=lastname]").valid() && Isvalid;
                                }
                                if(j$(currentcomp).find("[id$=email]").length ==1){
                                    if(Isvalid == undefined)
                                        Isvalid = j$(currentcomp).find("[id$=email]").valid();
                                    else
                                        Isvalid = j$(currentcomp).find("[id$=email]").valid() && Isvalid;
                                }
                                if(j$(currentcomp).find("[id$=address1]").length ==1){
                                    if(Isvalid == undefined)
                                        Isvalid = j$(currentcomp).find("[id$=address1]").valid();
                                    else
                                        Isvalid = j$(currentcomp).find("[id$=address1]").valid() && Isvalid;
                                }
                                if(j$(currentcomp).find("[id$=city]").length ==1){
                                    if(Isvalid == undefined)
                                        Isvalid = j$(currentcomp).find("[id$=city]").valid();
                                    else
                                        Isvalid = j$(currentcomp).find("[id$=city]").valid() && Isvalid;
                                }
                                if(j$(currentcomp).find("[id$=country]").length ==1){
                                    if(Isvalid == undefined)
                                        Isvalid = j$(currentcomp).find("[id$=country]").valid();
                                    else
                                        Isvalid = j$(currentcomp).find("[id$=country]").valid() && Isvalid;
                                }
                                if(j$(currentcomp).find("[id$=State_Province__c]").length ==1){
                                    if(Isvalid == undefined)
                                        Isvalid = j$(currentcomp).find("[id$=State_Province__c]").valid();
                                    else
                                        Isvalid = j$(currentcomp).find("[id$=State_Province__c]").valid() && Isvalid;
                                }
                                if(j$(currentcomp).find("[id$=ppnumber]").length ==1){
                                    if(Isvalid == undefined)
                                        Isvalid = j$(currentcomp).find("[id$=ppnumber]").valid();
                                    else
                                        Isvalid = j$(currentcomp).find("[id$=ppnumber]").valid() && Isvalid;
                                }
                                
                                if(j$(currentcomp).find("[id$=dealerCompanyName]").length ==1){
                                    if(Isvalid == undefined)
                                        Isvalid = j$(currentcomp).find("[id$=dealerCompanyName]").valid();
                                    else
                                        Isvalid = j$(currentcomp).find("[id$=dealerCompanyName]").valid() && Isvalid;
                                }
                                
                                if(j$(currentcomp).find("[id$=postalcode]").length ==1){
                                    if(Isvalid == undefined)
                                        Isvalid = j$(currentcomp).find("[id$=postalcode]").valid();
                                    else
                                        Isvalid = j$(currentcomp).find("[id$=postalcode]").valid() && Isvalid;
                                }
                                if(j$(currentcomp).find("[id$=jobfunction]").length ==1){
                                    var tempjob = j$(currentcomp).find("[id$=jobfunction]").valid();
                                    if(tempjob == false){
                                        j$(currentcomp).find("[id$=jobfunction]").addClass('selecterror');
                                    }else{
                                        j$(currentcomp).find("[id$=jobfunction]").removeClass('selecterror');
                                    }
                                    if(Isvalid == undefined){
                                        Isvalid = tempjob;
                                    }else{
                                        Isvalid = tempjob && Isvalid;
                                    }
                                } 
                                
                                var ppType = false;
                                if(j$(currentcomp).find("table[id$=PPT]").find('input[type=radio]:enabled').length > 0){
                                    j$(currentcomp).find("table[id$=PPT]").find('input[type=radio]:enabled').each(function(){
                                        if(j$(this).is(':checked')){
                                            ppType = true;
                                        }
                                    });
                                    
                                    if(!ppType){
                                        Isvalid = false;
                                        j$(currentcomp).find("table[id$=PPT]").addClass('selecterror');
                                    }
                                    else{
                                        j$(currentcomp).find("table[id$=PPT]").removeClass('selecterror');
                                    }
                                }
                                
                                var spType = false;
                                if(j$(currentcomp).find("table[id$=SPT]").find('input[type=radio]:enabled').length > 0 ){
                                    j$(currentcomp).find("table[id$=SPT]").find('input[type=radio]:enabled').each(function(){
                                        if(j$(this).is(':checked')){
                                            spType = true;
                                        }
                                    });
                                    if(j$(currentcomp).find("[id$=spnumber]:enabled").val() == ''|| j$(currentcomp).find("[id$=spnumber]:enabled").val() == null){
                                        spType = true;
                                    }
                                    if(!spType){
                                        Isvalid = false;
                                        j$(currentcomp).find("table[id$=SPT]").addClass('selecterror');
                                    }
                                    else{
                                        j$(currentcomp).find("table[id$=SPT]").removeClass('selecterror');
                                    }
                                }
                                
                                if(Isvalid){
                                    if(param1)
                                        ifSuccessCallSave(param);
                                    else
                                        buttonfunctionCorporate(this);
                                }else{
                                    var html = "";
                                    var errordiv = currenttabtoshowerror();
                                    j$(currentcomp).find('.kvh-show-errormsg-here').append(errordiv);
                                    if(j$(currentcomp).find('[id$=dealerCompanyName]').hasClass("error") && (j$(currentcomp).find("[id$=dealerCompanyName]").valid() == false)){
                                        html += "<li>Dealer/Distributor Company Name Required</li>";
                                    }
                                    if(param =='dealercomponent'){
                                        if(j$(currentcomp).find('[id$=ppnumber]').hasClass("error") && (j$(currentcomp).find("[id$=ppnumber]").valid() == false)){
                                            html += "<li>Company Phone Number Required</li>";
                                        }
                                        if(!ppType){
                                            html += "<li>Select Company Phone Type</li>";
                                        } 
                                    }
                                    if(j$(currentcomp).find('[id$=firstname]').hasClass("error") && (j$(currentcomp).find("[id$=firstname]").valid() == false)){
                                        html += "<li>First/Given Name Required</li>";
                                    }
                                    if(j$(currentcomp).find('[id$=lastname]').hasClass("error") && (j$(currentcomp).find("[id$=lastname]").valid() == false)){
                                        html += "<li>Last/Family Name/Surname Required</li>";
                                    }
                                    if(j$(currentcomp).find('[id$=email]').hasClass("error") && (j$(currentcomp).find("[id$=email]").valid() == false)){
                                        html += "<li>Valid Email Required</li>";
                                    }
                                    if(param !='dealercomponent'){
                                        if(j$(currentcomp).find('[id$=ppnumber]').hasClass("error") && (j$(currentcomp).find("[id$=ppnumber]").valid() == false)){
                                            html += "<li>Primary Phone Number Required</li>";
                                        }
                                        if(!ppType){
                                            html += "<li>Select Primary Phone Type</li>";
                                        }
                                    }
                                    if(!spType){
                                        if(param =='dealercomponent'){
                                            html += "<li>Select Contact Phone Type</li>";
                                        }else{
                                            html += "<li>Select Secondary Phone Type</li>";
                                        }
                                    }
                                    if(j$(currentcomp).find('[id$=jobfunction]').hasClass("error") && (j$(currentcomp).find("[id$=jobfunction]").valid() == false)){
                                        html += "<li>Select Role</li>";
                                    }
                                    if(j$(currentcomp).find('[id$=address1]').hasClass("error") && (j$(currentcomp).find("[id$=address1]").valid() == false)){
                                        html += "<li>Address 1 Required</li>";
                                    }
                                    if(j$(currentcomp).find('[id$=city]').hasClass("error") && (j$(currentcomp).find("[id$=city]").valid() == false)){
                                        html += "<li>City Required</li>";
                                    }
                                    if(j$(currentcomp).find('[id$=postalcode]').hasClass("error") && (j$(currentcomp).find("[id$=postalcode]").valid() == false)){
                                        html += "<li>Postal/Zip Code Required</li>";
                                    }
                                    if(j$(currentcomp).find('[id$=country]').hasClass("error") && (j$(currentcomp).find("[id$=country]").valid() == false)){
                                        html += "<li>Select Country</li>";
                                    }
                                    if(j$(currentcomp).find('[id$=State_Province__c]').hasClass("error") && (j$(currentcomp).find("[id$=State_Province__c]").valid() == false)){
                                        html += "<li>Select State/Province/Territory</li>";
                                    }
                                    j$(currentcomp).find('.error-box').find('.bullets').html(html);
                                    j$(currentcomp).find('.error-box').show();
                                }
                            }else{
                                if(param1)
                                    ifSuccessCallSave(param);
                                else
                                    buttonfunctionCorporate(this);
                            }
                        }
                    
                    
                    
                    function ifSuccessCallSave(param){
                        var currentcomp = j$("[id$="+param+"]");
                        j$(currentcomp).find('.selecterror').removeClass('selecterror');
                        j$(currentcomp).find('.error').removeClass("error");
                        j$(currentcomp).find('.kvh-show-errormsg-here').empty();
                        showLoadingDiv();
                        //check here current tab and call appropriate method, pass next tab id 
                        if(param == 'Act_subscriber')
                            callSubscriberSave('Authorized_Representatives');
                        if(param == 'contactcomponent')
                            callcontactFromAccountSetup('Authorized_Representatives');
                        if(param == 'dealercomponent')
                            callDealerSave('System_Type');
                        if(param == 'billingComponent')
                            callBillingSave('Invoicing_Preferences');
                        if(param == 'individualcomponent') 
                            callcontactFromInstallationType('Invoicing_Preferences'); 
                        if(param == 'ActivationGroupEmail') 
                            callcontactFromAccountSetupGrp('Invoicing_Preferences');
                        if(param == 'onboardcontactcomponent') 
                            callcontactFromInstallationType('Antenna_System_Support_Contact');
                        if(param == 'antennasystemsupportcontactcomponent') 
                            callcontactFromInstallationType('Dealer_Distributor_Info');
                        
                    }            
                    <!-- script Onboard Contact navigate Back Start -->     
                        function validateOnboardContactBack(){
                            showLoadingDiv();
                            btnSelectednavigateBack('Vessel_Details'); 
                        }
                    <!-- script Onboard Contact navigate Back End -->
                        
                        <!-- script Onboard Contact navigate Next Start -->     
                            function validateOnboardContactNext(){
                                var isvalid = false;
                                if(!isvalid){
                                    showLoadingDiv();
                                    callcontactFromInstallationType('Antenna_System_Support_Contact');
                                }
                            }
                    
                    <!-- script Onboard Contact navigate Next End --> 
                        
                        <!-- script Antenna System Support Contact navigate Back Start -->     
                            function validateAntennaSystemSupportContactBack(){
                                showLoadingDiv();
                                if(j$("[id$='Onboard_Contact']").length ==1){
                                    j$("[id$='Vessel_Details_cell']").show();
                                    btnSelectednavigateBack('Onboard_Contact'); 
                                }
                                else{
                                    btnSelectednavigateBack('Vessel_Details'); 
                                }
                                
                            }
                    <!-- script Antenna System Support Contact navigate Back End --> 
                        
                        <!-- script Antenna System Support Contact navigate Next Start -->     
                            function validateAntennaSystemSupportContactNext(){
                                var isvalid = false;
                                if(!isvalid){
                                    showLoadingDiv();
                                    callcontactFromInstallationType('Antenna_System_Support_Contact');
                                }
                            }
                    <!-- script Antenna System Support Contact navigate Next End --> 
                        
                        function generateRequired(){ 
                            j$('span:has(span.req), p:has(span.req), div:has(span.req)').children('input[type=text], select').addClass('requiredRedMarkFld');
                        }
